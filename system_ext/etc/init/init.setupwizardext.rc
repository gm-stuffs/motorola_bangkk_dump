on boot
    # Enable Decision Point screen during setup flow
    setprop setupwizard.feature.baseline_setupwizard_enabled true
    # Default mobile data to false during setup flow
    setprop ro.com.android.prov_mobiledata false
    # Mobile data activation timeout dialog
    setprop ro.setupwizard.loading_subscriptions_timeout_millis 720000
    # set setupwizard theme to GlifV4
    setprop setupwizard.theme glif_v4_light
    # Update Phenotype sync timeout to 1min
    setprop setupwizard.phenotype_sync_timeout_ms 60000
